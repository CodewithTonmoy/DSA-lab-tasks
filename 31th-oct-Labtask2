#include <iostream>
#include <stack>

int main() {
    std::stack<int> myStack;

    
    std::cout << "Input some elements onto the stack:" << std::endl;
    int n, element;
    std::cin >> n;
    for (int i = 0; i < n; i++) {
        std::cin >> element;
        myStack.push(element);
    }

    std::cout << "Stack elements are:";
    while (!myStack.empty()) {
        std::cout << " " << myStack.top();
        myStack.pop();
    }
    std::cout << std::endl;

  
    std::cout << "Find and remove the lowest element:" << std::endl;

    if (n > 0) {
        std::cin >> element; 
        int lowestElement = element;
        
        for (int i = 1; i < n; i++) {
            std::cin >> element;
            if (element < lowestElement) {
                lowestElement = element;
            }
        }
        
        std::cout << "Lowest element is: " << lowestElement << std::endl;
    } else {
        std::cout << "Stack is empty." << std::endl;
    }

    return 0;
}
